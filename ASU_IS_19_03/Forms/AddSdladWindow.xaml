<Window x:Class="ASU_IS_19_03.Forms.AddSdladWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASU_IS_19_03.Forms"
        mc:Ignorable="d"
        Title="AddSdladWindow" 
        SizeToContent="WidthAndHeight">
    
    <Grid>
        <StackPanel Margin="10, 30 ,10,50">
            <StackPanel  Orientation="Horizontal">
                <TextBlock Margin="10">Укажите адрес склада</TextBlock>
                <TextBox  Margin="10"
                    MinWidth="150"
                    x:Name="textBoxAdres"></TextBox>
            </StackPanel>
            <Button x:Name="btAdd" Content="Добавить в  БД"/>
        </StackPanel>
    </Grid>
</Window>
